{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Couchbase": {
    "ConnectionString": "couchbase://localhost",
    "Username": "<couchbaseUsername>",
    "Password": "<couchbasePassword>",
    "BucketName": "twitchchat"
  },
