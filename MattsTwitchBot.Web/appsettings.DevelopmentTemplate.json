    "Username": "<twitch username>",
    "OauthKey": "oauth:<twitch oauthkey>",
    "ApiClientId": "<twitch api client id>",
    "ApiClientSecret": "<twitch api client secret>"
  },
  "BearerToken": "<random characters of your choice>"
}
